package day2;

public class Employee {
	
	int id;
	String name;
	double salary;
	
	
	
	public Employee(int id, String name, double salary) {
		super();
		this.id = id;
		this.name = name;
		this.salary = salary;
	}



	public double incrementSalary(){
		return this.salary = this.salary + (this.salary * .1);
	}

}
