package day2.inhert;

class Animal {

	public void speak() {
		System.out.println("Animal Speak");
	}

}

class Tiger extends Animal {
	public void speak() {
		System.out.println("Tiger Speak");
	}
}

public class Test {
	public static void main(String[] args) {
		
		Animal animal = new Tiger();
		animal.speak();
		
		Tiger tiger = animal;
		
		

	}
}
