
public class GarbageCollectionDemo {

	public static void main(String[] args) {

		String str= "";

		for(int i=0;i<100000000;i++){
			
			str = str + i;
			
			
			try {
				Thread.sleep(2);
			} catch (InterruptedException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
		}

	}

}
