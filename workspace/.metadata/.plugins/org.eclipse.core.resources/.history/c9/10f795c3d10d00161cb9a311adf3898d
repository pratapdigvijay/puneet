
public class NewFeatures {

	public static void main(String[] args) {
		System.out.println(add(5,4,3));

	}
	
	
	public static int add(int ...x){
		int total = 0;
		for(int value : x){
			total+=value;
		}
		return total;
	}
	/*public static int add(int x, int y, int z){
		return x + y + z;
	}*/

}
