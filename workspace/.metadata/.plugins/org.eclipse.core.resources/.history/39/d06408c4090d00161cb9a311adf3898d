package day4.exceptions;

import day2.Employee;

public class ExceptionsDemo {
	
	public static void m1(){
		try{
			//Guarded Region
		}
		catch(Exception e){
			//Handling
		}
		
		
		throw new Exception();
	}
	
	public static void main(String[] args) {
		Employee emp = null;
		
		emp.getSalary();
	}

}
